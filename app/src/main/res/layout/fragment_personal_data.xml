<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="a.b.fragment.PersonalDataFragment">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="150dp">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_personal_data_bg" />

        <ImageButton
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            android:background="@drawable/ic_user_logo_defalut"
            android:scaleType="fitCenter" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|bottom"
            android:text="156****6666"
            android:textColor="#ffffff"
            android:textSize="16sp" />
    </FrameLayout>

    <RelativeLayout
        android:id="@+id/rl_personal_data_sex"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:clickable="true">

        <ImageView
            android:id="@+id/iv_personal_data_sex"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_margin="16dp"
            android:background="@drawable/ic_sex" />

        <TextView
            android:id="@+id/tv_personal_data_sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="46dp"
            android:clickable="false"
            android:focusable="false"
            android:text="性别"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_set1"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/tongyong_jiantou" />

        <TextView
            android:id="@+id/tv_personal_data_sex_display"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="13dp"
            android:layout_toLeftOf="@id/iv_set1"
            android:clickable="false"
            android:focusable="false"
            android:text="男"
            android:textSize="16sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_personal_data_birthday"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:clickable="true">

        <ImageView
            android:id="@+id/iv_personal_data_birthday"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_margin="16dp"
            android:background="@drawable/ic_birthday" />

        <TextView
            android:id="@+id/tv_personal_data_birthday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="46dp"
            android:clickable="false"
            android:focusable="false"
            android:text="生日"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_set2"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/tongyong_jiantou" />

        <TextView
            android:id="@+id/tv_personal_data_birthday_display"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="13dp"
            android:layout_toLeftOf="@id/iv_set2"
            android:clickable="false"
            android:focusable="false"
            android:text="1992/12/12"
            android:textSize="16sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_personal_data_constellation"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:clickable="true">

        <ImageView
            android:id="@+id/iv_personal_data_constellation"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_margin="16dp"
            android:background="@drawable/ic_constellation" />

        <TextView
            android:id="@+id/tv_personal_data_constellation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="46dp"
            android:clickable="false"
            android:focusable="false"
            android:text="星座"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_set3"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/tongyong_jiantou" />

        <TextView
            android:id="@+id/tv_personal_data_constellation_display"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="13dp"
            android:layout_toLeftOf="@id/iv_set3"
            android:clickable="false"
            android:focusable="false"
            android:text="摩羯座"
            android:textSize="16sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_personal_data_job"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:clickable="true">

        <ImageView
            android:id="@+id/iv_personal_data_job"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_margin="16dp"
            android:background="@drawable/ic_carrer" />

        <TextView
            android:id="@+id/tv_personal_data_job"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="46dp"
            android:clickable="false"
            android:focusable="false"
            android:text="职业"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_set4"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/tongyong_jiantou" />

        <TextView
            android:id="@+id/tv_personal_data_job_display"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="13dp"
            android:layout_toLeftOf="@id/iv_set4"
            android:clickable="false"
            android:focusable="false"
            android:text="程序员"
            android:textSize="16sp" />


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_personal_data_hobbies"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:clickable="true">

        <ImageView
            android:id="@+id/iv_personal_data_hobbies"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_margin="16dp"
            android:background="@drawable/ic_hobby" />

        <TextView
            android:id="@+id/tv_personal_data_hobbies"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="46dp"
            android:clickable="false"
            android:focusable="false"
            android:text="兴趣爱好"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/iv_set5"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="8dp"
            android:clickable="false"
            android:focusable="false"
            android:src="@drawable/tongyong_jiantou" />

        <TextView
            android:id="@+id/tv_personal_data_hobbies_display"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="13dp"
            android:layout_toLeftOf="@id/iv_set5"
            android:clickable="false"
            android:focusable="false"
            android:text="码代码"
            android:textSize="16sp" />


    </RelativeLayout>


</LinearLayout>
